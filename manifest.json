{
    "name": "doDizzy",
    "manifest_version": 2,
    "version": "1.0",
    "description": "Emulation of dizzy of the website",
    "permissions": [
        "https://*/*"
        ],
    "icons" : {
        "16": "doDizzy-small.png",
        "48": "doDizzy-aver.png",
        "128": "doDizzy-large.png"
    },

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"],
            "run_at": "document_end"
        }
    ]
}